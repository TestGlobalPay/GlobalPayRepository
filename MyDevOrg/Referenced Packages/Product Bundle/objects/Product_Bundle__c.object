    <webLinks>
        <fullName>Generate_Product</fullName>
        <availability>online</availability>
        <description>Generate a product from Bundle, so Sales can add it as single line item on opportunity or quote.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate Product</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)} 

if({!Product_Bundle__c.Has_Line_Item__c}&gt;0) { 

 if(confirm(&apos;This is optional. You can go to opportunity or quote to add this bundle directly.\n\nDo you want to continue?&apos;)){

   var sitePrefix = &apos;{!$Site.Prefix}&apos;!=null ? &apos;{!$Site.Prefix}&apos; : &apos;&apos;;

   //Construct VF URL 
   var queryString = new Array(); 
   queryString[0] = &apos;{!Product_Bundle__c.Id}&apos;; 
   queryString[1] = &apos;CreateProduct&apos;; 
   queryString[2] = &apos;none&apos;; 

